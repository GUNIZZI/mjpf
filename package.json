{
    "name": "mjpf",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:prod": "vite --mode production",
        "build": "tsc && vite build --mode production --outDir @dist/prod",
        "build:dev": "tsc && vite build --mode development --outDir @dist/dev",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "generate": "npm run build:dev && npm run build",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d @dist/prod"
    },
    "dependencies": {
        "firebase": "^11.0.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.52.1",
